{
  "name": "vscode-study.lenses",
  "description": "Explore JavaScript using an interactive tour based on CER best practises",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "main": "./dist/ext-src/extension.js",
  "publisher": "yoshi",
  "contributes": {
    "commands": [
      {
        "command": "study.lenses.annotate",
        "title": "Annotate",
        "category": "study.lenses"
      },
      {
        "command": "study.lenses.argument-picker",
        "title": "Argument Picker",
        "category": "study.lenses"
      },
      {
        "command": "study.lenses.comment-slots",
        "title": "Comment Slots",
        "category": "study.lenses"
      },
      {
        "command": "study.lenses.pseudify",
        "title": "Pseudify",
        "category": "study.lenses"
      },
      {
        "command": "study.lenses.parsons",
        "title": "Parsons",
        "category": "study.lenses"
      },
      {
        "command": "study.lenses.present",
        "title": "Present",
        "category": "study.lenses"
      },
      {
        "command": "study.lenses.flowchart",
        "title": "Flowchart",
        "category": "study.lenses"
      },
      {
        "command": "study.lenses.pick-flowchart",
        "title": "Pick Flowchart",
        "category": "study.lenses"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "study.lenses",
          "group": "navigation"
        }
      ],
      "study.lenses": [
        {
          "command": "study.lenses.annotate",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "study.lenses.argument-picker",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "study.lenses.parsons",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "study.lenses.comment-slots",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "study.lenses.pseudify",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "study.lenses.flowchart",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "study.lenses.pick-flowchart",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "study.lenses.present",
          "group": "navigation",
          "when": "resourceFilename =~ /.presentation.md/i || resourceFilename =~ /.slides.md/i"
        }
      ]
    },
    "submenus": [
      {
        "id": "study.lenses",
        "label": "Study Lenses"
      }
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --output-hashing none && tsc -p tsconfig.extension.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "vsce package --yarn",
    "vscode:prepublish": "npm run build && tsc -p tsconfig.extension.json"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.2.4",
    "@angular/cdk": "^15.2.7",
    "@angular/common": "^15.2.4",
    "@angular/compiler": "^15.2.4",
    "@angular/core": "^15.2.4",
    "@angular/forms": "^15.2.4",
    "@angular/material": "^15.2.7",
    "@angular/platform-browser": "^15.2.4",
    "@angular/platform-browser-dynamic": "^15.2.4",
    "@angular/router": "^15.2.4",
    "acorn": "^8.8.2",
    "acorn-walk": "^8.2.0",
    "escodegen": "^2.0.0",
    "js2flowchart": "^1.3.4",
    "reveal.js": "^4.5.0",
    "rxjs": "~7.8.0",
    "snippet-highlight": "^1.0.2",
    "tslib": "^2.5.0",
    "vscode-test": "^1.6.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.4",
    "@angular/language-service": "^15.2.4",
    "@types/node": "^18.15.9",
    "@types/vscode": "^1.76.0",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5",
    "vsce": "^2.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/4gray/vscode-webview-angular.git"
  }
}
